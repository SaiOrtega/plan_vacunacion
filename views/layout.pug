doctype html
html(lang="es")
     head
       meta(charset="UTF-8")
       meta(name="viewport", content="width=device-width, initial-scale=1")
       script(defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js")
       script(src="https://kit.fontawesome.com/a5eb905d06.js" crossorigin="anonymous")
       script(src="https://cdn.jsdelivr.net/npm/sweetalert2@11")    
       script(src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.js")
       block scripts
       link(href="https://cdn.jsdelivr.net/npm/@sweetalert2/theme-dark@4/dark.css" rel="stylesheet")
       link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css")
       link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)  
       link(href='/styles/style-navbar.css' rel='stylesheet')
      
       block link
       
       title #{Titulo}
     body.position-relative.bg-body-tertiary.pb-5.pt-0(data-bs-theme="secondary")
       nav.navbar.navbar-expand-sm.navbar-primary.bg-primary(aria-label='Third navbar example')
        
         .container-fluid
           #navbarsExample03.collapse.navbar-collapse
             ul.navbar-nav.me-auto.mb-2.mb-sm-0
               li.nav-item.dropdown
                 a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' aria-expanded='false') Distribución
                 ul.dropdown-menu
                   li
                     a.dropdown-item(href="/envioVacunas") Envio de Lotes a Provincias
                   li
                     a.dropdown-item(href='/envioVacunasCentros') Envio de Lotes a Centros
               li.nav-item.dropdown
                 a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' aria-expanded='false') Stocks
                 ul.dropdown-menu
                   li
                     a.dropdown-item(href='/modCompra') Stock Nación
                   li
                     a.dropdown-item(href='/depositoProvinciaStock') Stock Provincia
                   li
                     a.dropdown-item(href='/centrosStock') Stock Centros
               li.nav-item.dropdown
                 a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' aria-expanded='false') Vacunas
                 ul.dropdown-menu
                   li
                     a.dropdown-item(href='/aplicacionVacuna') Aplicar Vacuna   
                   li
                    hr.dropdown-divider                      
                   li
                     
                     a.dropdown-item(href='/compra') Comprar Lotes Vacunas
                   li
                    a.dropdown-item(href='/crearVacuna') Alta Vacuna    
                   li
                    a.dropdown-item(href='/vacunasDescarte') Descartar Lotes  
                   li                       
                    a.dropdown-item(href='/reasignarLotes') Reasignar Lotes 
               li.nav-item.dropdown
                 a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' aria-expanded='false') Gestiones
                 ul.dropdown-menu
                   li
                     a.dropdown-item(href='/agregarCentro') Agregar un Centro 
                   li
                     a.dropdown-item(href='/agregarDepoNacion') Agregar un Deposito Nacional
                   li
                     a.dropdown-item(href='/agregarDepoProvincia') Agregar un Deposito Provincial
               li.nav-item.dropdown
                 a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' aria-expanded='false') Informes
                 ul.dropdown-menu
                   li
                     a.dropdown-item(href='/vacunasAplicadas') Vacunas Aplicadas
                   li
                     a.dropdown-item(href='/vacunasDescartadas') Vacunas Descartadas
                   li
                     a.dropdown-item(href='/lotesProvedor') Lote-Provedor de Vacunas

             .d-flex.gap-3
               ul.navbar-nav.me-auto.mb-2.mb-md-0
                li.nav-item
                   button.nav-link(id="inicio") Inicio 
                li.nav-item
                  a.nav-link(href=loginlogoutLink) #{loginlogoutName}

                
       block content
       
script. 
  function inicio() {  
    window.location.href = '/';
  }
  const botonInicio= document.getElementById('inicio');
  botonInicio.addEventListener('click', inicio);




  